{
  "name": "Node.js Hello World",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-vscode.vscode-typescript-next"
      ]
    }
  },
  "appPort": ["3000:3000"],
  "forwardPorts": [3000],
  "portsAttributes": {
    "3000": {
      "label": "App",
      "onAutoForward": "openBrowser",
      "visibility": "public"
    }
  },
  "onCreateCommand": "npm install",
  "postAttachCommand": "npm start"
}